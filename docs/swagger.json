                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Example"
                            }
                        }
                    }
                }
            },
            "post": {
                "description": "Create a new example with the provided information",
